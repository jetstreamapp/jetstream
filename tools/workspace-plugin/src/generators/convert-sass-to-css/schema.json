{
  "$schema": "http://json-schema.org/schema",
  "$id": "convert-sass-to-css",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library/Project name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "filename": {
      "type": "string",
      "description": "Input filename with full path"
    }
  },
  "required": [
    "name",
    "filename"
  ]
}
