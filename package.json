  "scripts": {
    "init:project": "ts-node ./scripts/init-project.ts",
    "nx": "nx",
    "start": "nx serve",
    "start:api": "nx serve api",
    "start:worker": "nx serve jetstream-worker",
    "start:landing": "nx serve landing",
    "start:docs": "nx serve docs",
    "start:electron": "nx serve electron-jetstream",
    "start:electron-worker": "nx build electron-worker --configuration=development --watch",
    "start:electron-preferences": "nx build electron-preferences --configuration=development --watch",
    "start:watch:electron": "nx run-many --target=build --projects=electron-worker,electron-preferences,electron-jetstream --configuration=development --watch --parallel --maxParallel=10",
    "start:ui:storybook": "nx run ui:storybook",
    "start:prod": "pm2-runtime start ecosystem.config.js --env production",
    "start:staging": "pm2-runtime start ecosystem.staging.config.js --env production",
    "start:e2e": "node dist/apps/api/main.js",
    "start:prod:debug": "pm2-runtime start ecosystem.config.js --env production --node-args=\"--inspect\"",
    "start:cron:inactive-account-warning": "node dist/apps/cron-tasks/inactive-account-warning.js",
    "start:cron:inactive-account-deletion": "node dist/apps/cron-tasks/inactive-account-deletion.js",
    "build": "cross-env NODE_ENV=production npm-run-all db:generate build:client build:api build:sw build:landing rollbar:upload-sourcemaps",
    "build:docker": "cross-env NODE_ENV=production npm-run-all db:generate build:client:docker build:api:docker build:sw build:landing",
    "build:electron": "cross-env NODE_ENV=production npm-run-all build:electron:jetstream-app build:electron:css build:electron:jetstream build:electron:worker build:electron:preferences",
    "build:electron:jetstream-app": "nx build jetstream --configuration=electron",
    "build:electron:jetstream": "nx build electron-jetstream --prod",
    "build:electron:worker": "nx build electron-worker --prod",
    "build:electron:preferences": "nx build electron-preferences --prod",
    "build:electron:css": "tailwindcss -o apps/electron/jetstream/src/assets/tailwind.css -c apps/electron/jetstream/tailwind.config.js --minify",
    "build:affected": "npx nx affected --target=build --parallel --max-parallel=10 --configuration=production",
    "build:test": "npm-run-all db:generate build:client:test build:api:test build:sw build:landing",
    "build:client": "nx build jetstream --prod",
    "build:client:docker": "nx build jetstream --configuration=docker",
    "build:client:stats": "nx build jetstream --prod --statsJson",
    "build:client:test": "nx build jetstream --configuration=test --verbose",
    "build:ui": "nx build ui --configuration=docker",
    "build:ui:test": "nx build ui --configuration=test",
    "build:api": "nx build api --prod",
    "build:api:docker": "nx build api --configuration=docker",
    "build:cron": "yarn db:generate && nx build cron-tasks --prod",
    "build:api:test": "nx build api --configuration=test",
    "build:sw": "nx build download-zip-sw --prod",
    "build:landing": "cross-env NODE_ENV=production nx run landing:export && yarn build:landing:sitemap",
    "build:landing:sitemap": "next-sitemap",
    "build:docs": "cross-env USE_SIMPLE_CSS_MINIFIER=true nx build docs",
    "build:storybook": "build-storybook -c libs/ui/.storybook -o dist/storybook -s node_modules/@salesforce-ux/design-system/assets/styles",
    "electron:dist:macos": "nx run electron-jetstream:make --platform=mac --arch=x64 --publishPolicy=onTagOrDraft",
    "electron:dist:macos-arm64": "nx run electron-jetstream:make --platform=mac --arch=arm64 --publishPolicy=onTagOrDraft",
    "electron:dist:win": "nx run electron-jetstream:make --platform=windows --arch=x64 --publishPolicy=onTagOrDraft",
    "electron:extract:asar-x86": "npx asar extract dist/executables/mac/Jetstream.app/Contents/Resources/app.asar dist/executables/mac/extracted",
    "electron:extract:asar-arm": "npx asar extract dist/executables/mac-arm64/Jetstream.app/Contents/Resources/app.asar dist/executables/mac-arm64/extracted",
    "release": "dotenv release-it --verbose",
    "release:build": "ts-node ./scripts/build-release.ts",
    "release:electron": "yarn build:electron && concurrently --group --restart-tries=3 \"npm:electron:dist:*\"",
    "pm2:link": "pm2-runtime link quhsrndp43edp7v urfb6rbocz587wt",
    "pm2:monitor": "pm2 monit",
    "bundle-analyzer:client": "npx webpack-bundle-analyzer dist/apps/jetstream/stats.json",
    "docs:index": "docker run -it -e APPLICATION_ID=21D7I5RB7N -e API_KEY=5a8e69d756ddc4a3067c7d9e7aecfc3b -e \"CONFIG=$(cat ./apps/docs/algolia-config.json | jq -r tostring)\" algolia/docsearch-scraper",
    "rollbar:upload-sourcemaps": "ts-node ./scripts/upload-source-maps.ts",
    "generate:data-table-css": "nx workspace-generator convert-sass-to-css ui --filename /src/lib/data-table/data-table-styles.scss",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "chmod +x prisma/seed.sh && prisma db seed",
    "icons:build": "npm-run-all icons:build:*",
    "icons:build:action": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/action ./node_modules/@salesforce-ux/design-system/assets/icons/action",
    "icons:build:custom": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/custom ./node_modules/@salesforce-ux/design-system/assets/icons/custom",
    "icons:build:doctype": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/doctype ./node_modules/@salesforce-ux/design-system/assets/icons/doctype",
    "icons:build:standard": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/standard ./node_modules/@salesforce-ux/design-system/assets/icons/standard",
    "icons:build:utility": "npx @svgr/cli --config-file .svgo-config.json -d ./libs/icon-factory/src/lib/icons/utility ./node_modules/@salesforce-ux/design-system/assets/icons/utility",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "playwright:test:with-server": "yarn start-server-and-test --expect 200 'yarn start:e2e' http://localhost:3333 'yarn playwright:test'",
    "playwright:test": "yarn playwright test src --config apps/jetstream-e2e/playwright.config.ts",
    "playwright:test:query-results": "yarn playwright test query-results.spec.ts --config apps/jetstream-e2e/playwright.config.ts --headed",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph"
  },
